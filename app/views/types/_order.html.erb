<% t.row do |tr| %>
  <%= tr.cell do %><%= market.name %><% end %>
  <%= tr.cell do %><%= market_order_range order.range %><% end %>
  <%= tr.cell do %><%= number_to_m3 order.volume_remain %><% end %>
  <%= tr.cell do %><%= number_to_isk order.price %><% end %>
  <%= tr.cell do %><%= order.location_name %><% end %>
  <%= tr.cell do %><%= distance_of_time_in_words Time.zone.now, order.expires_at %><% end %>
  <%= tr.cell do %><%= local_time order.issued_at, format: :db %><% end %>
<% end %>
